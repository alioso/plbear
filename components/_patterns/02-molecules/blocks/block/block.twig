{% if content %}
  <{{ element ?: "div" }}{{ attributes.addClass(classes) }}>

  {{ title_prefix }}
  {% if label %}
    <{{ label_element ?: "h2" }}{{ title_attributes.addClass(title_classes) }}>{{ label }}</{{ label_element ?: "h2" }}>
  {% endif %}
  {{ title_suffix }}

  {% block content %}
    {{ content }}
  {% endblock %}
  </{{ element ?: "div" }}>
{% endif %}